<h1>Список тестов</h1>
<%= link_to t('.create_test'), new_admin_test_path %>

<table class="table table-striped">
  <tr class="table-primary">
    <th><%= t('.title') %></th>
    <th><%= t('.category') %></th>
    <th><%= t('.author') %></th>
    <th><%= t('.level') %></th>
    <th><%= t('.count_questions') %></th>
    <th colspan="3"><%= t('.actions') %></th>
  </tr>
  <%= render @tests %>
</table>

<%= link_to t('.back'), :root %>

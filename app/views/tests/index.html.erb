<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @tests.each do |test| %>
    <tr>
      <td><%= test.title %></td>
      <td><%= test.level %></td>
      <td><%= link_to "Show", test %></td>
      <td><%= link_to "Edit", edit_test_path(test) %></td>
      <td><%= link_to "Destroy", test, method: :delete, data: { confirm: "Are you sure?" } %></td>
    </tr>
  <% end %>
  </tbody>
</table>